{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "class-methods-use-this": "off",
      "no-param-reassign": "off",
      "consistent-return": "off",
      "no-named-as-default": "off",
      "camelcase": "off",
      "import/no-cycle": 0,
      "import/no-extraneous-dependencies": 0
    }
}
